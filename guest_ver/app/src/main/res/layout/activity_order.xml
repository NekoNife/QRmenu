<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/backbt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".order">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="411dp"
        android:layout_height="137dp"
        android:background="#E28DAF"
        android:backgroundTint="#E28DAF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/shopid2"
        android:layout_width="212dp"
        android:layout_height="51dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="100dp"
        android:fontFamily="@font/noto_sans_bold"
        android:gravity="center"
        android:text="가계이름"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#353535"
        tools:context=".order">

        <Button
            android:id="@+id/orderbt1"
            android:layout_width="181dp"
            android:layout_height="161dp"
            android:layout_marginStart="22dp"
            android:layout_marginEnd="295dp"
            android:layout_marginBottom="30dp"
            android:backgroundTint="#DADADA"
            android:fontFamily="@font/noto_sans_bold"
            android:scrollbarAlwaysDrawHorizontalTrack="false"
            android:scrollbarAlwaysDrawVerticalTrack="false"
            android:scrollHorizontally="false"
            android:text="1"
            android:textColor="#353535"
            android:textColorHint="#353535"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/orderbt3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/orderbt3"
            android:layout_width="181dp"
            android:layout_height="161dp"
            android:layout_marginStart="22dp"
            android:layout_marginEnd="295dp"
            android:layout_marginBottom="30dp"
            android:backgroundTint="#DADADA"
            android:fontFamily="@font/noto_sans_bold"
            android:text="3"
            android:textColor="#353535"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/orderbt5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/orderbt5"
            android:layout_width="181dp"
            android:layout_height="161dp"
            android:layout_marginStart="22dp"
            android:layout_marginEnd="295dp"
            android:layout_marginBottom="30dp"
            android:backgroundTint="#DADADA"
            android:fontFamily="@font/noto_sans_bold"
            android:text="5"
            android:textColor="#353535"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/orderbt2"
            android:layout_width="181dp"
            android:layout_height="161dp"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#DADADA"
            android:fontFamily="@font/noto_sans_bold"
            android:text="2"
            android:textColor="#353535"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/orderbt1"
            app:layout_constraintTop_toTopOf="@+id/orderbt1" />

        <Button
            android:id="@+id/orderbt4"
            android:layout_width="181dp"
            android:layout_height="161dp"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="16dp"
            android:backgroundTint="#DADADA"
            android:fontFamily="@font/noto_sans_bold"
            android:text="4"
            android:textColor="#353535"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/orderbt1"
            app:layout_constraintTop_toTopOf="@+id/orderbt3" />

        <Button
            android:id="@+id/orderbt6"
            android:layout_width="181dp"
            android:layout_height="161dp"
            android:layout_marginStart="11dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="30dp"
            android:backgroundTint="#DADADA"
            android:fontFamily="@font/noto_sans_bold"
            android:text="6"
            android:textColor="#353535"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/orderbt1"
            app:layout_constraintTop_toTopOf="@+id/orderbt5" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="411dp"
            android:layout_height="137dp"
            android:background="#E28DAF"
            android:backgroundTint="#E28DAF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/shopid"
            android:layout_width="212dp"
            android:layout_height="51dp"
            android:layout_marginStart="99dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="100dp"
            android:fontFamily="@font/noto_sans_bold"
            android:gravity="center"
            android:text="가계이름"
            android:textColor="@color/white"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>