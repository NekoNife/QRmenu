        app:layout_constraintTop_toTopOf="@+id/orderbt3" />

    <Button
        android:id="@+id/orderbt6"
        android:layout_width="181dp"
        android:layout_height="161dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#DADADA"
        android:text="6번"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/orderbt1"
        app:layout_constraintTop_toTopOf="@+id/orderbt5" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="72dp"
        android:layout_height="28dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="309dp"
        android:text="1번 테이블"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shopid" />

    <TextView
        android:id="@+id/textView5"
