<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".order">

    <Button
        android:id="@+id/orderbt1"
        android:layout_width="181dp"
        android:layout_height="161dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="295dp"
        android:backgroundTint="#DADADA"
        android:text="1번"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/orderbt3"
        android:layout_width="181dp"
        android:layout_height="161dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="295dp"
        android:backgroundTint="#DADADA"
        android:text="3번"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/orderbt5"
        android:layout_width="181dp"
        android:layout_height="161dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="295dp"
        android:backgroundTint="#DADADA"
        android:text="3번"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/orderbt2"
        android:layout_width="181dp"
        android:layout_height="161dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#DADADA"
        android:text="2번"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/orderbt1"
        app:layout_constraintTop_toTopOf="@+id/orderbt1" />

    <Button
        android:id="@+id/orderbt4"
        android:layout_width="181dp"
        android:layout_height="161dp"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#DADADA"
        android:text="4번"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
